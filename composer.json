{
    "name": "philiagus/dataprovider",
    "description": "DataProvider for PHP Unit testing",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "dataprovider"
    ],
    "authors": [
        {
            "name": "Andreas Eicher",
            "email": "php@philiagus.de",
            "role": "lead"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "support": {
        "issues": "https://github.com/philiagus/dataprovider/issues"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "ext-xdebug": "*"
    },
    "autoload": {
        "psr-4": {
            "Philiagus\\DataProvider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Philiagus\\DataProvider\\Test\\": "tests"
        }
    }
}
